{
  "cli": {
    "version": ">= 9.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "preview": {
      "developmentClient": false,
      "channel": "preview",
      "android": { "buildType": "apk" }
    },
    "internal": {
      "distribution": "internal",
      "channel": "internal",
      "android": { "buildType": "apk" }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "ios": { "resourceClass": "m-medium" },
      "android": { "gradleCommand": ":app:bundleRelease" }
    }
  },
  "submit": {
    "production": {}
  }
}