{
  "name": "mweb",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3002",
    "smoke": "node scripts/smoke.js",
    "figma:tokens": "node ../../scripts/figma-extract-tokens.mjs && echo tokens done",
    "figma:generate": "node ../../scripts/figma-generate-mweb.mjs && echo generate done",
    "figma:assets": "node ../../scripts/figma-assets-sync.mjs && echo assets done",
    "figma:sync": "FIGMA_FILE_KEY=$FIGMA_FILE_KEY FIGMA_TOKEN=$FIGMA_TOKEN pnpm figma:tokens && pnpm figma:generate && pnpm figma:assets"
  },
  "dependencies": {
    "swiper": "^11.1.10",
    "gsap": "3.12.5",
    "lucide-vue-next": "^0.544.0",
    "pinia": "2.2.4",
    "vue": "3.5.13",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@playwright/test": "1.48.2",
    "playwright": "1.48.2",
    "@vitejs/plugin-vue": "5.1.2",
    "autoprefixer": "10.4.20",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.2",
    "vite": "5.4.3"
  }
}
